{
  "name": "{{ .Title }}",
{{- with .Params.Providers }}
  "providers": {{ . | jsonify }},
{{- end }}
{{- with .Params.Services }}
  "services": {{ . | jsonify }},
{{- end }}
{{- with .Params.Domains }}
  "domains": {{ . | jsonify }},
{{- end }}
{{- with .Params.Categories }}
  "categories": {{ . | jsonify }},
{{- end }}
{{- with .Params.Features }}
  "features": {{ . | jsonify }},
{{- end }}
{{- with .Params.Labels }}
  "labels": {{ . | jsonify }},
{{- end }}
  "permalink": "{{ .RelPermalink | replaceRE "\\.json$" "" }}"
}
